#include "stdafx.h"

#include "iostream"

using namespace std;

int main( )

{

	int i,j;	bool flag;

	for( i=2;i<=100;i++){

		flag=false;

	for(j=2;j<i;j++){

		if(i%j==0)

			flag=true;

			}

			if(flag==false)

		cout<<i<<" ";

		}	

	return 0;

}
